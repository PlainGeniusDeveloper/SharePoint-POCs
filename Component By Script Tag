"Accordion": (content) => {
        // Initialize an empty string to hold all accordion items' HTML
        let accordionHTML = '';

        // Loop through each accordion item in the list
        content.tabAccordionList.forEach(accordionItem => {
            // Get the accordion item template
            let accordionItemTemplate = getTemplate('accordion-item-template');

            // Bind the title and description to the accordion item template
            accordionHTML += bindDataToTemplate(accordionItemTemplate, {
                title: accordionItem.title,       // Assuming your JSON has 'title'
                description: accordionItem.description // Assuming your JSON has 'description'
            });
        });

        // Return the complete HTML for all accordion items
        return accordionHTML;
    },
