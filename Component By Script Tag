"Accordion": (content) => {
        // Generate HTML for each accordion item using createAccordionItems
        let accordionHTML = '';
        
        // Iterate through each accordion in the list and bind its content
        content.tabAccordionList.forEach(accordionItem => {
            accordionHTML += getTemplate('accordion-item-template', {
                title: accordionItem.title,       // Assuming your data has 'title' for accordion header
                description: accordionItem.description // Assuming your data has 'description' for accordion content
            });
        });

        return accordionHTML; // Return the generated accordion HTML content for all items
    },
